    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "chokidar": "^3.5.3",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
